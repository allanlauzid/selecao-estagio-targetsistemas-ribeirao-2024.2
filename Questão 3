public class Soma {

    public static void main(String[] args) {
        int INDICE = 12, SOMA = 0, K = 1;

        while (K < INDICE) {
            K = K + 1;
            SOMA = SOMA + K;
        }

        System.out.println("SOMA = " + SOMA);
    }
}

//Ao final do processamento, qual será o valor da variável SOMA? -> 77
